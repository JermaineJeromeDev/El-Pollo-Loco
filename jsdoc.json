{
    "source": {
        "include": [
        "classes",
        "js",
        "levels"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "readme": "./README.md"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}